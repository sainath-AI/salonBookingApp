<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.masai.sainath.salonbookingapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/download"
        android:label="@string/app_name"
        android:roundIcon="@drawable/download"
        android:supportsRtl="true"
        android:theme="@style/Theme.SalonBookingApp">

        <activity
            android:name=".view.activity.ListOfSalons"
            android:exported="true" />
        <activity
            android:name=".view.activity.PaymentDetails"
            android:exported="false" />
        <activity
            android:name=".view.activity.SlotActivty"
            android:exported="true" />
        <activity
            android:name=".view.activity.SaloonDetailsActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".view.activity.UserActivtiy"
            android:exported="true" />
        <activity
            android:name=".view.activity.AdminActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.MainActivity"
            android:exported="true" />
        <activity
            android:name=".view.activity.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>